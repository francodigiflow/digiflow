					<xsl:if test="DTE/Documento/Encabezado/IdDoc/TipoDTE!=52">
						<font face="Arial" size="1">
							<strong>CEDIBLE</strong>
						</font>
					</xsl:if>
				</td>
			</tr>
		</table>
