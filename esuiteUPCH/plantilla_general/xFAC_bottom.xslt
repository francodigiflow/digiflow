	<xsl:template name="TipoDespacho">
		<xsl:choose>
			<xsl:when test="DTE/Documento/Encabezado/IdDoc/IndTraslado='0'">
				<xsl:value-of select="'Sin indicador Venta'"/>
			</xsl:when>
			<xsl:when test="DTE/Documento/Encabezado/IdDoc/IndTraslado='1'">
				<xsl:value-of select="'Operacion Constituye Venta'"/>
			</xsl:when>
			<xsl:when test="DTE/Documento/Encabezado/IdDoc/IndTraslado='2'">
				<xsl:value-of select="'Ventas por Efectuar'"/>
			</xsl:when>
			<xsl:when test="DTE/Documento/Encabezado/IdDoc/IndTraslado='3'">
				<xsl:value-of select="'Consignaciones'"/>
			</xsl:when>
			<xsl:when test="DTE/Documento/Encabezado/IdDoc/IndTraslado='4'">
				<xsl:value-of select="'Entrega Gratuita'"/>
			</xsl:when>
			<xsl:when test="DTE/Documento/Encabezado/IdDoc/IndTraslado='5'">
				<xsl:value-of select="'Traslados internos'"/>
			</xsl:when>
			<xsl:when test="DTE/Documento/Encabezado/IdDoc/IndTraslado='6'">
				<xsl:value-of select="'Otros Traslados no venta'"/>
			</xsl:when>
			<xsl:when test="DTE/Documento/Encabezado/IdDoc/IndTraslado='7'">
				<xsl:value-of select="'Guia de  Devolucion'"/>
			</xsl:when>
			<xsl:when test="DTE/Documento/Encabezado/IdDoc/IndTraslado='8'">
				<xsl:value-of select="'Traslado para exportación. (no venta)'"/>
			</xsl:when>
			<xsl:when test="DTE/Documento/Encabezado/IdDoc/IndTraslado='9'">
				<xsl:value-of select="'Venta para Exportación'"/>
			</xsl:when>
		</xsl:choose>
	</xsl:template>
	<xsl:template name="timbre">

		<xsl:variable name="rue">
			<xsl:value-of select="substring-before(DTE/Documento/Encabezado/Emisor/RUTEmisor|DTE/Documento/Encabezado/Emisor/RUTEmisor, '-')"/>
		</xsl:variable>
		<xsl:value-of select="'E'"/>
		<xsl:call-template name="nombreTed">
			<xsl:with-param name="text" select="$rue"/>
			<xsl:with-param name="largo" select="'9'"/>
		</xsl:call-template>
		<xsl:value-of select="'T'"/>
		<xsl:call-template name="nombreTed">
			<xsl:with-param name="text" select="DTE/Documento/Encabezado/IdDoc/TipoDTE|DTE/Documento/Encabezado/IdDoc/TipoDTE|a"/>
			<xsl:with-param name="largo" select="'3'"/>
		</xsl:call-template>
		<xsl:value-of select="'F'"/>
		<xsl:call-template name="nombreTed">
			<xsl:with-param name="text" select="DTE/Documento/Encabezado/IdDoc/Folio|DTE/Documento/Encabezado/IdDoc/Folio|a"/>
			<xsl:with-param name="largo" select="'10'"/>
		</xsl:call-template>
		<xsl:value-of select="'.jpg'"/>
	</xsl:template>

	<xsl:template name="timbreEx">

		<xsl:variable name="rue">
			<xsl:value-of select="substring-before(DTE/Exportaciones/Encabezado/Emisor/RUTEmisor|DTE/Exportaciones/Encabezado/Emisor/RUTEmisor, '-')"/>
		</xsl:variable>
		<xsl:value-of select="'E'"/>
		<xsl:call-template name="nombreTed">
			<xsl:with-param name="text" select="$rue"/>
			<xsl:with-param name="largo" select="'9'"/>
		</xsl:call-template>
		<xsl:value-of select="'T'"/>
		<xsl:call-template name="nombreTed">
			<xsl:with-param name="text" select="DTE/Exportaciones/Encabezado/IdDoc/TipoDTE|DTE/Exportaciones/Encabezado/IdDoc/TipoDTE|a"/>
			<xsl:with-param name="largo" select="'3'"/>
		</xsl:call-template>
		<xsl:value-of select="'F'"/>
		<xsl:call-template name="nombreTed">
			<xsl:with-param name="text" select="DTE/Exportaciones/Encabezado/IdDoc/Folio|DTE/Exportaciones/Encabezado/IdDoc/Folio|a"/>
			<xsl:with-param name="largo" select="'10'"/>
		</xsl:call-template>
		<xsl:value-of select="'.jpg'"/>
	</xsl:template>

	<xsl:template name="nombreTed">
		<xsl:param name="text"/>
		<xsl:param name="largo" select="0"/>
		<xsl:variable name="largo_actual" select="string-length($text)"/>
		<xsl:choose>
			<xsl:when test="$largo_actual >= $largo">
				<xsl:value-of select="$text"/>
			</xsl:when>
			<xsl:otherwise>
				<xsl:variable name="prefix" select="concat('0', $text)"/>
				<xsl:call-template name="nombreTed">
					<xsl:with-param name="text" select="$prefix"/>
					<xsl:with-param name="largo" select="$largo"/>
				</xsl:call-template>
			</xsl:otherwise>
		</xsl:choose>
	</xsl:template>

